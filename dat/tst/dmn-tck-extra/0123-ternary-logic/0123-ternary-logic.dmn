        <inputEntry id="ie1r1"><text>true</text></inputEntry>
        <inputEntry id="ie2r1"><text>true</text></inputEntry>
        <outputEntry id="oe1r1"><text>true</text></outputEntry>
      </rule>
      <rule>
        <inputEntry id="ie1r2"><text>true</text></inputEntry>
        <inputEntry id="ie2r2"><text>null</text></inputEntry>
