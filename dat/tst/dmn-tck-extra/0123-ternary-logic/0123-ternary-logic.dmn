        <outputEntry id="oe1r3"><text>null</text></outputEntry>
      </rule>
      <rule>
        <inputEntry id="ie1r4"><text>false</text></inputEntry>
        <inputEntry id="ie2r4"><text>true</text></inputEntry>
        <outputEntry id="oe1r4"><text>false</text></outputEntry>
      </rule>
      <rule>
        <inputEntry id="ie1r5"><text>true</text></inputEntry>
        <inputEntry id="ie2r5"><text>false</text></inputEntry>
        <outputEntry id="oe1r5"><text>false</text></outputEntry>
