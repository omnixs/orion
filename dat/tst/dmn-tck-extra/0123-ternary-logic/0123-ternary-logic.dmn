<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="ternary-logic-test" name="Ternary Logic Test" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="d1" name="TernaryAnd">
    <decisionTable id="dt1">
      <input id="i1" label="A">
        <inputExpression id="ie1" typeRef="boolean"><text>A</text></inputExpression>
      </input>
      <input id="i2" label="B">
