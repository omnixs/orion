      <input id="i1" label="A">
        <inputExpression id="ie1" typeRef="boolean"><text>A</text></inputExpression>
      </input>
      <input id="i2" label="B">
        <inputExpression id="ie2" typeRef="boolean"><text>B</text></inputExpression>
      </input>
      <output id="o1" label="Result" typeRef="boolean"/>
      <rule>
        <inputEntry id="ie1r1"><text>true</text></inputEntry>
